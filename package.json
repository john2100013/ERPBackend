{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "vercel-build": "tsc",
    "migrate": "ts-node -e \"import('./src/database/migrations').then(m => m.createTables())\"",
    "seed": "ts-node -e \"import('./src/database/migrations').then(m => m.seedDatabase())\"",
    "migrate:seed": "npm run migrate && npm run seed",
    "sync-schema": "ts-node src/scripts/syncSchema.ts",
    "sync-schema:apply": "ts-node src/scripts/syncSchema.ts --apply",
    "dump-neon-schema": "ts-node scripts/dump-neon-schema.ts",
    "sync-from-schema": "ts-node scripts/sync-from-schema.ts",
    "sync-from-schema:apply": "ts-node scripts/sync-from-schema.ts --apply",
    "validate-schema": "ts-node scripts/validate-schema-match.ts",
    "verify-schema": "ts-node scripts/verify-final-schema.ts",
    "recreate-db": "ts-node scripts/recreate-from-neon-direct.ts",
    "migrate:neon": "node run_neon_migration.js",
    "migrate:local": "node run_local_migration.js",
    "migrate:all": "node run_all_migrations.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
